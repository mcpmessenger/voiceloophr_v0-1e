# VoiceLoopHR Production Configuration
# Copy this file to .env.local and add your actual API keys

# OpenAI API Key (for Whisper STT, GPT-4, and TTS)
OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs API Key (for TTS - optional, OpenAI TTS is primary)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Optional: Voice ID for ElevenLabs (default: Rachel voice)
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# AWS Configuration (Required for Textract)
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1

# AWS S3 Bucket for Textract processing
AWS_S3_BUCKET=voiceloophr-hr-documents-20241201

# Production Environment
NODE_ENV=production
NEXT_PUBLIC_APP_ENV=production
