# VoiceLoop HR Environment Variables
# Copy to .env.local and add your credentials

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Google OAuth
GOOGLE_OAUTH_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=your_client_secret

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional APIs
OPENAI_API_KEY=sk-your-openai-key
ELEVENLABS_API_KEY=your_elevenlabs_key
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# MCP (Model Context Protocol) Configuration
OPENAI_MCP_SERVER_URL=http://localhost:3000/api/mcp/openai
CALENDAR_MCP_SERVER_URL=http://localhost:3000/api/mcp/calendar
MCP_TIMEOUT=30000
MCP_RETRIES=3
MCP_CLIENT_TIMEOUT=30000
MCP_CLIENT_RETRIES=3

# Google Calendar Integration
GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback

# Microsoft Outlook Integration
MICROSOFT_CLIENT_ID=your_microsoft_client_id
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret
MICROSOFT_REDIRECT_URI=http://localhost:3000/auth/microsoft/callback
MICROSOFT_TENANT_ID=common